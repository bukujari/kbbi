{"arti":["<b>ke&#xB7;luh</b> <i>n</i> ungkapan yg keluar krn perasaan susah (krn menderita sesuatu yg berat, kesakitan, dsb);<br>--<b> kesah</b> segala ucapan yg terlahir krn kesusahan (kepedihan dsb);<br><b>ber&#xB7;ke&#xB7;luh</b> <i>v</i> mengeluh;<br>~<b> kesah</b> melahirkan perasaan susah dng mengaduh, menarik napas, dsb;<br><b>me&#xB7;nge&#xB7;luh</b> <i>v</i> menyatakan susah (krn penderitaan, kesakitan, kekecewaan, dsb): <i>meskipun tugas itu sangat berat, tetapi tidak seorang pun yg ~;<br></i><b>me&#xB7;nge&#xB7;luh&#xB7;kan</b> <i>v</i> mengeluh tt sesuatu: <i>ia ~ perlakuan yg tidak adil;<br></i><b>ter&#xB7;ke&#xB7;luh</b> <i>a</i> tidak sengaja mengeluh;<br><b>pe&#xB7;nge&#xB7;luh</b> <i>n</i> orang yg suka mengeluh;<br><b>ke&#xB7;luh&#xB7;an</b> <i>n</i> apa yg dikeluhkan; keluh kesah"],"similar":[{"uri":"/kelu","keyword":"kelu"},{"uri":"/kelua","keyword":"kelua"},{"uri":"/keluai","keyword":"keluai"},{"uri":"/keluak","keyword":"keluak"},{"uri":"/keluan","keyword":"keluan"},{"uri":"/keluang","keyword":"keluang"},{"uri":"/keluangsa","keyword":"keluangsa"},{"uri":"/keluar","keyword":"keluar"},{"uri":"/keluarga","keyword":"keluarga"},{"uri":"/kelubak","keyword":"kelubak"},{"uri":"/kelubi","keyword":"kelubi"},{"uri":"/kelubung","keyword":"kelubung"},{"uri":"/keluburan","keyword":"keluburan"},{"uri":"/kelui","keyword":"kelui"},{"uri":"/keluih","keyword":"keluih"},{"uri":"/keluk","keyword":"keluk"},{"uri":"/kelukup","keyword":"kelukup"},{"uri":"/kelukur","keyword":"kelukur"},{"uri":"/keluli","keyword":"keluli"},{"uri":"/kelulu","keyword":"kelulu"},{"uri":"/kelulus","keyword":"kelulus"},{"uri":"/kelulut","keyword":"kelulut"},{"uri":"/kelumit","keyword":"kelumit"},{"uri":"/kelumpang","keyword":"kelumpang"},{"uri":"/kelumun","keyword":"kelumun"},{"uri":"/kelun","keyword":"kelun"},{"uri":"/keluna","keyword":"keluna"},{"uri":"/kelunak","keyword":"kelunak"},{"uri":"/kelung","keyword":"kelung"},{"uri":"/kelupas","keyword":"kelupas"}]}
